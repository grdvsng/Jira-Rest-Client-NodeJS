{
  "tags": 
  {
    "allowUnknownTags": true,
    "dictionaries": 
    [
       "jsdoc"
      ,"closure"
    ]
  },
  "source": 
  {
    "include": 
    [  
      "./bin/",
      "./tests/"
    ],
    "exclude": 
    [  
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": 
  {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./doc/",
    "recurse": false,
    "tutorials": ""
  },
  
  "plugins": ["plugins/markdown"],
  "markdown": 
  {
    "tags": ["foo", "bar"]
  },

  "templates": 
  {
    "cleverLinks": true
    ,"monospaceLinks": true
  }
}